# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def isPalindrome(self, head: ListNode) -> bool:
        l = []
        p = head
        while p!=None:
            l.append(p)
            p = p.next
        i,j = 0,len(l)-1
        while i<j:
            if l[i].val == l[j].val:
                i += 1
                j -= 1
            else:
                return False
        return True
